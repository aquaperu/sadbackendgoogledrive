{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
    {
      "include": "/assets",
      "outDir": "dist/src",
      "watchAssets": true
    },
    {
      "include": "/especificacionesTecnicas",
      "outDir": "dist/src",
      "watchAssets": true
    },
    {
      "include": "toolsdocx/services/styles",
      "outDir": "dist/src",
      "watchAssets": true
    },
    {
      "include": "docs/services/styles",
      "outDir": "dist/src",
      "watchAssets": true
    },
    {
      "include": "/audio",
      "outDir": "dist/src",
      "watchAssets": true
    }
  ],
  "watchAssets": true
  }
}
